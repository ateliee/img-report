{
  "name": "img-report",
  "version": "0.3.0",
  "description": "Extract image differences between directories and output HTML reports",
  "keywords": [
    "img-report",
    "report",
    "Resemble",
    "Material",
    "diff"
  ],
  "main": "app.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "start": "node bin/img-report diff ./examples/assets ./examples/dist -f -r",
    "serve": "node bin/img-report serve -s ./examples/assets/ -d ./examples/dist/",
    "build": "node bin/img-report build -s ./examples/assets/ -d ./examples/dist/",
    "lint": "eslint src/**/* lib/**/* bin/**/*",
    "lint:fix": "eslint src/**/* lib/**/* bin/**/* --fix",
    "test": "mocha --recursive",
    "travis:setup": "travis encrypt $([[ $(cat ~/.npmrc) =~ _authToken=(.*)$ ]] && echo ${BASH_REMATCH[1]}) --add deploy.api_key"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ateliee/img-report.git"
  },
  "bin": {
    "img-report": "bin/img-report.js"
  },
  "engines": {
    "node": ">= 8.16.2"
  },
  "author": "ateliee <info@ateliee.com> (https://ateliee.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ateliee/img-report/issues"
  },
  "homepage": "https://github.com/ateliee/img-report/",
  "dependencies": {
    "@babel/core": "7.7.5",
    "@babel/plugin-proposal-class-properties": "7.7.4",
    "@babel/preset-env": "7.7.6",
    "@babel/preset-react": "7.7.4",
    "@babel/register": "7.7.4",
    "@material-ui/core": "4.7.2",
    "@material-ui/icons": "4.5.1",
    "babel-loader": "8.0.6",
    "clean-webpack-plugin": "3.0.0",
    "commander": "4.0.1",
    "copy-webpack-plugin": "5.1.0",
    "css-loader": "3.3.0",
    "date-utils": "1.2.21",
    "fs-extra": "8.1.0",
    "html-webpack-plugin": "3.2.0",
    "lodash": "4.17.15",
    "prop-types": "15.7.2",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-lazyload": "2.6.5",
    "resemblejs": "3.2.3",
    "sass-loader": "8.0.0",
    "webpack": "4.41.2",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.9.0"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "eslint": "6.7.2",
    "eslint-plugin-react": "7.17.0",
    "mocha": "6.2.2",
    "mocha-sinon": "2.1.0"
  }
}
